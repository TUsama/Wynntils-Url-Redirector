{
  "schemaVersion": 1,
  "id": "wynntils_url_redirector",
  "version": "${version}",
  "name": "wynntils_url_redirector",
  "description": "Redirect the download links of Wynntils to a responsive mirror.",
  "authors": [
    "Clefal"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT License",
  "icon": "assets/wynntils_url_redirector/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.clefal.wynntils_url_redirector"
    ],
    "client": [
      "com.clefal.wynntils_url_redirector_client"
    ]
  },
  "mixins": [
    "wynntils_url_redirector.mixins.json",
    {
      "config": "wynntils_url_redirector.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "wynntils": "\u003e\u003d2.3.0",
    "fabricloader": ">=0.16.5",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}